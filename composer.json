{
    "name": "siteko/filament-restic-backups",
    "description": "Restic backups management for Laravel 12 + Filament.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "filament",
        "restic",
        "backups",
        "s3"
    ],
    "homepage": "https://github.com/siteko-net/filament-restic-backups",
    "support": {
        "issues": "https://github.com/siteko-net/filament-restic-backups/issues",
        "source": "https://github.com/siteko-net/filament-restic-backups"
    },
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php": "^3.0",
        "filament/filament": "^4.0",
        "laravel/framework": "^12.0"
    },
    "autoload": {
        "psr-4": {
            "Siteko\\FilamentResticBackups\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Siteko\\FilamentResticBackups\\ResticBackupsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
